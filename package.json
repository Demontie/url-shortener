{
  "name": "url-shortener",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/main.ts",
    "build": "tsc",
    "start": "node dist/main.js",
    "lint:fix": "biome lint --write",
    "biome:format": "biome format --write --unsafe",
    "biome:check": "biome check --write",
    "test": "vitest run",
    "test:unit": "vitest run --config vitest.unit.config.ts",
    "test:integration": "vitest run --config vitest.integration.config.ts",
    "test:unit:coverage": "vitest run --config vitest.unit.config.ts --coverage",
    "test:integration:coverage": "vitest run --config vitest.integration.config.ts --coverage",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "db:drizzle:migrate": "drizzle-kit migrate",
    "db:drizzle:generate": "drizzle-kit generate",
    "db:drizzle:push": "drizzle-kit push",
    "db:drizzle:pull": "drizzle-kit pull",
    "db:drizzle:studio": "drizzle-kit studio --port 8989 --verbose"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "amqplib": "^0.10.9",
    "cassandra-driver": "^4.8.0",
    "dotenv": "^17.2.3",
    "eventemitter2": "^6.4.9",
    "fastify": "^5.6.2",
    "hashids": "^2.3.0",
    "redis": "^5.10.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.7",
    "@types/amqplib": "^0.10.8",
    "@types/supertest": "^6.0.3",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "supertest": "^7.1.4",
    "testcontainers": "^11.8.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  }
}
